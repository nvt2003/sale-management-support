@model Objects.Models.InvoiceDetail

@{
    ViewData["Title"] = "Invoice Detail";
}

<h2>Invoice Detail</h2>

<div>
    <strong>Invoice Detail ID:</strong> @Model.InvoiceDetailId
</div>
<div>
    <strong>Invoice ID:</strong> @Model.InvoiceId
</div>
